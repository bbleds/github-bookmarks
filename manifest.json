{
  "manifest_version": 2,
  "name": "GitHub Bookmarks",
  "description": "Show GitHub starred repos",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/bookmark.png",
    "default_popup": "window.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://api.github.com/"
  ]
}
